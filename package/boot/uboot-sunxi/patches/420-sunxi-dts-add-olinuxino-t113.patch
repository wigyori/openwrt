+++ b/arch/arm/dts/Makefile
@@ -706,6 +706,7 @@ dtb-$(CONFIG_MACH_SUN8I_R528) += \
 	sun8i-t113s-mangopi-mqdual-t113.dtb \
 	sun8i-t113s-myir-myd-yt113x-emmc.dtb \
 	sun8i-t113s-myir-myd-yt113x-spi.dtb \
